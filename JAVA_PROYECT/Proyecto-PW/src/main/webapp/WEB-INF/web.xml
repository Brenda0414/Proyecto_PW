<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>registro</servlet-name>
        <jsp-file>/registro.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>iniciosesion</servlet-name>
        <jsp-file>/iniciosesion.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>CU</servlet-name>
        <jsp-file>/CU.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>perfil</servlet-name>
        <jsp-file>/perfil.jsp</jsp-file>
    </servlet>
    
      
    <servlet-mapping>
        <servlet-name>registro</servlet-name>
        <url-pattern>/registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>iniciosesion</servlet-name>
        <url-pattern>/iniciosesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CU</servlet-name>
        <url-pattern>/CU</url-pattern>
    </servlet-mapping>
  
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/registro/Register</url-pattern>
    </servlet-mapping>
   <servlet-mapping>
        <servlet-name>Session</servlet-name>
        <url-pattern>/CU/Session</url-pattern>
    </servlet-mapping>
   <servlet-mapping>
        <servlet-name>perfil</servlet-name>
        <url-pattern>/perfil</url-pattern>
    </servlet-mapping> 
     
   
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file> index.jsp </welcome-file>
        <welcome-file> CU.jsp </welcome-file>
    </welcome-file-list>
</web-app>
